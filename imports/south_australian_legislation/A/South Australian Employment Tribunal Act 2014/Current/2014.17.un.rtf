or{\*\bkmkend id56dad385_31a1_4623_b5a4_f1769a11c138_8}\par
{\*\bkmkstart id564612a3_e686_4658_abcf_8d544fb38574_3}\pard\plain\f0\fs23\expnd0\kerning0\cf0\sb120\sa0
\sl240\slmult1\ql\keep\widctlpar\li1588\ri0\fi-794\tqc\tx1191\tx1588\tab (b)\tab failing to 
