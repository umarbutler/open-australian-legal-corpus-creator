appropriate.{\*\bkmkend idf185789d_33da_46b5_a2ba_a6454ae9250e_f}\par
{\*\bkmkstart id8304b611_f2d8_4dd0_ad86_5cc6359df70c_c}\pard\plain\f0\fs23\expnd0\kerning0\cf0\sb120\sa0
\sl240\slmult1\ql\keep\keepn\widctlpar\li794\ri0\fi-794\tqc\tx397\tx794\tab (6)\tab The Chief 
