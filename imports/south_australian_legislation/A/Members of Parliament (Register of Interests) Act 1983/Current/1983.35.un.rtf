and{\*\bkmkend id85b9de1f_34eb_4d1b_97be_dad417ac5f82}\par
\pard\plain\f0\fs23\expnd0\kerning0\cf0\sb120\sa0
\sl240\slmult1\ql\keep\widctlpar\li1588\ri0\fi-794\tqc\tx1191\tx1588\tab (f)\tab the information 
required by 
{\field\fldedit {\*\fldinst {HYPERLINK  \\l "id9ba1c390_b027_4925_b2c6_8ad8a810cd40" }{\*\datafield }}{\fldrslt {subsection\'a0(3)
}}}.\par
