to which this section applies; and{\*\bkmkend idc6800fc1_2c16_4706_8000_0833aeddb8c9}\par
{\*\bkmkstart id0efb9038_b5aa_472d_8480_f2d9e432ff08}\pard\plain\f0\fs23\expnd0\kerning0\cf0\sb120\sa0
\sl240\slmult1\ql\keep\widctlpar\li1588\ri0\fi-794\tqc\tx1191\tx1588\tab (b)\tab any dealing 
